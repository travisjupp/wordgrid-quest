{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./*"],
      "@components/*": ["src/components/*"],
      "@contexts/*": ["src/contexts/*"],
      "@utils/*": ["src/utils/*"],
      "@fonts/*": ["assets/fonts/*"],
      "@images/*": ["assets/images/*"],
      "@custom-types/*": ["src/types/*"],
      "@theme/*": ["src/theme/*"],
      "@prototype/*": ["prototype/*"],
      "@features/*": ["src/features/*"]
    },
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
